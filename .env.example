# AgentIorg Environment Configuration
# Copy this to .env and fill in your values

# OpenClaw Gateway
OPENCLAW_GATEWAY_URL=http://localhost:3333
OPENCLAW_API_TOKEN=your_token_here

# Notifications
TELEGRAM_ENABLED=true
TELEGRAM_CHAT_ID=your_chat_id

# Search APIs
BRAVE_API_KEY=your_brave_api_key

# Email (for future automation)
# SMTP_HOST=smtp.gmail.com
# SMTP_PORT=587
# SMTP_USER=your_email@gmail.com
# SMTP_PASS=your_app_password

# Database (for future scaling)
# DATABASE_URL=postgresql://user:pass@localhost:5432/agentiorg

# Analytics
# GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Content Platforms
# YOUTUBE_API_KEY=your_youtube_api_key
# LINKEDIN_ACCESS_TOKEN=your_linkedin_token
